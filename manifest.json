{
  "manifest_version": 3,
  "name": "List All Tabs",
  "version": "4.0.0",
  "description": "show a list of all open tabs",
  "short_name": "List All Tabs",
  "author": "Lior Shtivelman",
  "permissions": [
    "tabs",
    "storage",
    "tabGroups"
  ],
  "options_page": "options.html",
  "action": {
    "default_icon": "images/dark_version.png",
    "default_popup": "index.html"
  },
  "icons": {
    "128": "icon_128.png"
  }
}
